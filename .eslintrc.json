{
    "extends": ["airbnb","prettier", "prettier/react"],
    "parser": "babel-eslint",
    "rules": {
        "jsx-a11y/anchor-is-valid": 0,
        "jsx-a11y/label-has-for": 0,
        "class-methods-use-this": 0,
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "prefer-promise-reject-errors": 0
    },
    "globals": {
        "localStorage": true,
        "document": true,
        "FormData":true
    }
}